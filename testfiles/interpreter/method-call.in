class Box = {
      var m : any;

      fun set(v : any) = {
      	  m := v;
      }

      fun get() = {
      	  return m;
      }
}

fun main(x : int, y : int) : int = {
    var b : Box := new Box();
    var bb : Box := new Box();
    var b1 : Box := new Box();


    b.set(x);
    bb.set(b1);
    b1.set(y);

    var r := b.get();
    // since type analysis is not implemented, we need to ascribe the result of
    // Box:get(), to be able to call .get() on it
    return r + (bb.get():Box).get();
}
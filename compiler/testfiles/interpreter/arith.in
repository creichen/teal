fun main(x : int, y : int) : int = {
    // x = 11
    // y = 8
    var s1 : int := x + y;
    if s1 != 19 { return 1; }

    var s2 : int := x * y;
    if s2 != 88 { return 2; }

    var s3 : int := x - y;
    if s3 != 3 { return 3; }

    var s4 : int := x / y;
    if s4 != 1 { return 4; }

    var s5 : int := x % y;
    if s5 != 3 { return 5; }

    var s6 : int := x == y;
    if s6 != 0 { return 6; }

    var s7 : int := x != y;
    if s7 != 1 { return 7; }

    var s8 : int := x < y;
    if s8 != 0 { return 8; }

    var s9 : int := x <= x;
    if s9 != 1 { return 9; }

    var s10 : int := x > y;
    if s10 != 1 { return 10; }

    var s11 : int := y >= y;
    if s11 != 1 { return 11; }

    var s12 : int := x > y and y > x;
    if s12 != 0 { return 12; }

    var s13 : int := x > y or y > x;
    if s13 != 1 { return 13; }

    return 0;

}